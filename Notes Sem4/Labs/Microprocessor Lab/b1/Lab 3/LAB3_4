   DATA SEGMENT
   NUM1 DW 1234H
   NUM2 DW 1234H
   SUM1 DW 0000H
   CARRY1 DW 0000H

   DATA ENDS

   CODE SEGMENT 
   ASSUME CS:CODE , DS:DATA
   START:
   MOV AX,DATA
   MOV DS,AX
   MOV AX, NUM1
   MOV BX, NUM2
   MOV CX, 00H
   SBB AX, BX

   MOV SUM1,AX
   SBB CARRY1,0
   MOV AX, 4CH
   INT 21H
   CODE ENDS

